{
  "dashboard": {
    "title": "checkout-service - Service Dashboard",
    "panels": [
      {
        "id": 1,
        "type": "row",
        "title": "SLO Metrics",
        "collapsed": false,
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "panels": []
      },
      {
        "id": 2,
        "title": "Availability SLO",
        "type": "gauge",
        "targets": [
          {
            "expr": "(sum(rate(http_requests_total{service=\"$service\",status!~\"5..\"}[5m]))) / (sum(rate(http_requests_total{service=\"$service\"}[5m]))) * 100",
            "legendFormat": "Availability %",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 1
        },
        "description": "Current availability vs 99.95% SLO target",
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 98.95,
                  "color": "yellow"
                },
                {
                  "value": 99.95,
                  "color": "green"
                }
              ]
            }
          }
        },
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 3,
        "title": "Latency-P95 SLO",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{service=\"$service\"}[5m])) * 1000",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"$service\"}[5m])) * 1000",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{service=\"$service\"}[5m])) * 1000",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 6,
          "y": 1
        },
        "description": "Latency percentiles (target: 99.0% under 500ms)",
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "green"
                },
                {
                  "value": 400.0,
                  "color": "yellow"
                },
                {
                  "value": 500,
                  "color": "red"
                }
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 4,
        "title": "Latency-P99 SLO",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{service=\"$service\"}[5m])) * 1000",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"$service\"}[5m])) * 1000",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{service=\"$service\"}[5m])) * 1000",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 8,
